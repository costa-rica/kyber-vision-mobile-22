{
  "expo": {
    "name": "kyber-vision-mobile-22",
    "slug": "kyber-vision-mobile-22",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "scheme": "kybervisionmobile22",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./src/assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.costarica.kybervisionmobile22",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./src/assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.costarica.kybervisionmobile22"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/expo-assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/expo-assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.485629947502-2e3i0ibrp9ec4e8r1frg8b0815knmlnh"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "713b5e86-35a4-47cc-beb8-6d404fe1b1f7"
      }
    },
    "owner": "costa-rica",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d9e867ce-4255-4722-87d4-2ba2c5779f90"
    }
  }
}
